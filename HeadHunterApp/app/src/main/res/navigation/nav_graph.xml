        tools:layout="@layout/fragment_favorites" >
        <action
            android:id="@+id/action_favoritesFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_favoritesFragment_to_responseFragment"
            app:destination="@id/responseFragment" />
        <action
            android:id="@+id/action_favoritesFragment_to_massageFragment"
            app:destination="@id/massageFragment" />
        <action
            android:id="@+id/action_favoritesFragment_to_entryEmailFragment"
            app:destination="@id/entryEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/responseFragment"
        android:name="com.example.headhunterapp.presentation.response.ResponseFragment"
        android:label="fragment_vacancies_page"
        tools:layout="@layout/fragment_response" >
        <action
            android:id="@+id/action_responseFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_responseFragment_to_entryEmailFragment"
            app:destination="@id/entryEmailFragment" />
        <action
            android:id="@+id/action_responseFragment_to_favoritesFragment"
            app:destination="@id/favoritesFragment" />
        <action
            android:id="@+id/action_responseFragment_to_massageFragment"
            app:destination="@id/massageFragment" />
    </fragment>
    <fragment
        android:id="@+id/massageFragment"
        android:name="com.example.headhunterapp.presentation.massagefragment.MassageFragment"
        android:label="MassageFragment" >
        <action
            android:id="@+id/action_massageFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_massageFragment_to_entryEmailFragment"
            app:destination="@id/entryEmailFragment" />
        <action
            android:id="@+id/action_massageFragment_to_responseFragment"
            app:destination="@id/responseFragment" />
        <action
            android:id="@+id/action_massageFragment_to_favoritesFragment"
            app:destination="@id/favoritesFragment" />
        <action
            android:id="@+id/action_massageFragment_to_responseFragment2"
            app:destination="@id/responseFragment" />
    </fragment>
