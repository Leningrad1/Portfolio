            app:destination="@id/massageFragment" />
        <action
            android:id="@+id/action_vacancyAllFragment_to_responseFragment"
            app:destination="@id/responseFragment" />
        <action
            android:id="@+id/action_vacancyAllFragment_to_entryEmailFragment"
            app:destination="@id/entryEmailFragment" />
        <action
            android:id="@+id/action_vacancyAllFragment_to_favoritesFragment"
            app:destination="@id/favoritesFragment" />
        <action
            android:id="@+id/action_vacancyAllFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>